{
  "name": "brown-university-styles",
  "description": "Colors, typography and other style utilities for building React-based applications at Brown University.",
  "author": "Colin Cummings <colin_cummings@brown.edu>",
  "version": "0.3.2",
  "private": true,
  "homepage": "https://bitbucket.brown.edu/projects/REACT/repos/brown-university-styles",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint src stories",
    "prettier": "prettier \"{src,stories,__{tests,mocks}__}/**/*.+(js|json|css)\"",
    "format:check": "npm run prettier -- --list-different",
    "format:fix": "npm run prettier -- --write",
    "validate": "npm-run-all lint format:check",
    "prebuild": "rimraf dist && npm run validate",
    "build": "webpack",
    "start": "start-storybook -p 8080"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@storybook/react": "^5.0.3",
    "babel-loader": "^8.0.5",
    "copy-to-clipboard": "^3.0.8",
    "css-loader": "^2.1.1",
    "eslint": "^5.15.1",
    "eslint-config-brown": "git+https://bitbucket.brown.edu/scm/node/eslint-config-brown.git#semver:^0.12",
    "file-loader": "^3.0.1",
    "mini-css-extract-plugin": "^0.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3"
  }
}
