{
  "name": "brown-university-styles",
  "description": "Colors, typography and other style utilities for building React-based applications at Brown University.",
  "author": "Colin Cummings <colin_cummings@brown.edu>",
  "version": "0.12.0",
  "private": true,
  "homepage": "https://github.com/BrownUniversity/brown-university-styles.git",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint src stories",
    "prettier": "prettier \"{src,stories,__{tests,mocks}__}/**/*.+(js|json|css)\"",
    "format:check": "npm run prettier -- --list-different",
    "format:fix": "npm run prettier -- --write",
    "validate": "npm-run-all lint format:check",
    "prebuild:docs": "rimraf docs",
    "build:docs": "build-storybook -o docs",
    "prebuild": "rimraf dist && npm run validate",
    "build": "webpack",
    "start": "start-storybook -p 8080"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@storybook/addon-a11y": "^6.0.28",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addons": "^6.0.28",
    "@storybook/react": "^6.5.13",
    "babel-loader": "^8.0.5",
    "copy-to-clipboard": "^3.0.8",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-brown": "git+https://github.com/BrownUniversity/eslint-config-brown.git#semver:^0.14",
    "file-loader": "^3.0.1",
    "mini-css-extract-plugin": "^0.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3"
  }
}
